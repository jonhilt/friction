@model Friction.WebVS.Features.Users.List.Model
@{
    ViewData["Title"] = "List";
}

<h1>Users</h1>

@foreach (var user in Model.Users)
{
    <table>
        <thead>
            <tr>
                <th>First Name</th>
                <th>Last Name</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
            </tr>
        </tbody>
    </table>
}
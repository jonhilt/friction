@model Friction.WebVS.Features.Users.Search.Model

<form asp-controller="User" asp-action="Search" method="get">
    <input name="term" type="text" />
    <input type="submit" value="Search" />
</form>

<h1>Search Results</h1>
<table class="table">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
        </tr>
    </thead>
    @foreach (var user in Model.Results)
    {
        <tr>
            <td>@user.FirstName</td>
            <td>@user.LastName</td>
        </tr>
    }
</table>